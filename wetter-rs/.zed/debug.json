// Project-local debug tasks
//
// For more documentation on how to configure debug tasks,
// see: https://zed.dev/docs/debugger
[
  {
    "label": "Probe-RS STLink",
    "adapter": "probe-rs",
    "chip": "STM32H750VB",
    "request": "launch",
    "cwd": "$ZED_WORKTREE_ROOT",
    "coreConfigs": [
      {
        "programBinary": "target/thumbv7em-none-eabihf/debug/wetter-rs",
      },
    ],
    "flashingConfig": {
      "flashingEnabled": true,
      "haltAfterReset": true,
    },
    "build": {
      "command": "cargo build",
    },
  },
]
